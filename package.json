{
	"name": "vscode-timer",
	"displayName": "vscode-timer",
	"publisher": "jan25",
	"repository": {
		"url": "https://github.com/jan25/vscode-timer"
	},
	"description": "VS Code Timer Extension for deadlines and taking breaks between coding sessions.",
	"version": "0.1.2",
	"icon": "assets/timer.png",
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vscode-timer.start",
		"onCommand:vscode-timer.cancel"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-timer.start",
				"title": "Timer: Start"
			},
			{
				"command": "vscode-timer.cancel",
				"title": "Timer: Cancel"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.57.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}